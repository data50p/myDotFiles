function fix () {
eval $1 $2 $4 $3
}

cd ~

fix ln -s .zshrc ~/DotFiles/zshrc
fix ln -s .bash_profile  ~/DotFiles/bash_profile
fix ln -s .bashrc_lars  ~/DotFiles/bashrc_lars
fix ln -s .emacs  ~/DotFiles/emacs
fix ln -s .emacs.d  ~/DotFiles/emacs.d
fix ln -s .emacs_custom_lars  ~/DotFiles/emacs_custom_lars
fix ln -s .emacs_lars  ~/DotFiles/emacs_lars
fix ln -s .gitconfig  ~/DotFiles/gitconfig
fix ln -s .gitconfig-mpb50p  ~/DotFiles/gitconfig-mpb50p
fix ln -s .oh-my-theme.json ~/DotFiles/oh-my-theme.json
fix ln -s .config/ghostty/config ~/DotFiles/config_ghostty_config
