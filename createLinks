function fix () {
eval $1 $2 $4 $3
}

fix ln -s .zshrc  /Users/lars/DotFiles/zshrc
fix ln -s .bash_profile  /Users/lars/DotFiles/bash_profile
fix ln -s .bashrc_lars  /Users/lars/DotFiles/bashrc_lars
fix ln -s .emacs  /Users/lars/DotFiles/emacs
fix ln -s .emacs.d  /Users/lars/DotFiles/emacs.d
fix ln -s .emacs_custom_lars  /Users/lars/DotFiles/emacs_custom_lars
fix ln -s .emacs_lars  /Users/lars/DotFiles/emacs_lars
fix ln -s .git-completion.bash  /Users/lars/DotFiles/git-completion.bash
fix ln -s .gitconfig  /Users/lars/DotFiles/gitconfig
fix ln -s .gitconfig-mpb50p  /Users/lars/DotFiles/gitconfig-mpb50p
fix ln -s .run-sync  /Users/lars/DotFiles/run-sync
fix ln -s .oh-my-theme.json /users/lars/DotFiles/oh-my-theme.json
